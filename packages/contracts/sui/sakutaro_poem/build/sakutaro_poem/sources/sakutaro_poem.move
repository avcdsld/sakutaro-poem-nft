    struct SakutaroPoem has key, store {
        id: UID,
        poem_id: u64,
        poem_title: String,
        poem_svg_base64: String,
        svg_ipfs_cid: String,
    }

    struct SakutaroPoemSupply has key {
        id: UID,
        total_supply: u64,
        max_supply: u64,
    }

    struct SAKUTARO_POEM has drop {}

    // ===== Events =====

    struct NFTMinted has copy, drop {
        object_id: ID,
        creator: address,
        name: String,
    }

    #[allow(unused_function,unused_variable)]
    fun init(otw: SAKUTARO_POEM, ctx: &mut TxContext) {
        let publisher = package::claim(otw, ctx);
        let keys = vector[
            utf8(b"name"),
            utf8(b"description"),
            utf8(b"image_url"),
            utf8(b"animation_url"),
            utf8(b"license"),
            utf8(b"creator"),
        ];
        let values = vector[
            utf8(b"{poem_title}"),
            utf8(b"Thirty-nine poems from Sakutaro Hagiwara's late self-selected collection \"Shukumei\" have been inscribed on Blockchain as full-onchain NFTs. The content of this NFT changes depending on the owner."),
            utf8(b"ipfs://{svg_ipfs_cid}"),
            utf8(b"{poem_svg_base64}"),
            utf8(b"CC-BY 4.0"),
            utf8(b"Ara"),
        ];
        let display = display::new_with_fields<SakutaroPoem>(
            &publisher, keys, values, ctx
        );
        display::update_version(&mut display);
        transfer::public_transfer(publisher, sender(ctx));
        transfer::public_transfer(display, sender(ctx));

        transfer::share_object(SakutaroPoemSupply {
            id: object::new(ctx),
            total_supply: 0,
            max_supply: 39,
        })
    }

    public fun get_poem_id(owner: address): u64 {
        return ((address::to_u256(owner) % 39) as u64)
    }

    public fun get_poem_title(poem_id: u64): String {
        return utf8(*vector::borrow(&POEM_TITLES, poem_id))
    }

    public fun get_poem_body(poem_id: u64): String {
        return utf8(*vector::borrow(&POEM_BODIES, poem_id))
    }

    public fun get_poem_svg_ipfs_cid(poem_id: u64): String {
        return utf8(*vector::borrow(&POEM_SVG_IPFS_CID, poem_id))
    }

    public fun get_poem_svg(poem_id: u64): String {
        let svg = utf8(b"");
        append_utf8(&mut svg, b"<svg width=\"400\" height=\"400\" viewBox=\"0, 0, 400, 400\" xmlns=\"http://www.w3.org/2000/svg\">");
        append_utf8(&mut svg, b"<defs><linearGradient id=\"grad1\" x1=\"0%\" y1=\"50%\"><stop offset=\"0%\" stop-color=\"#0f2350\">");
        append_utf8(&mut svg, b"<animate id=\"a1\" attributeName=\"stop-color\" values=\"#0f2350; #6a5acd\" begin=\"0; a2.end\" dur=\"3s\" />");
        append_utf8(&mut svg, b"<animate id=\"a2\" attributeName=\"stop-color\" values=\"#6a5acd; #0f2350\" begin=\"a1.end\" dur=\"3s\" /></stop><stop offset=\"100%\" stop-color=\"#6a5acd\" >");
        append_utf8(&mut svg, b"<animate id=\"a3\" attributeName=\"stop-color\" values=\"#6a5acd; #0f2350\" begin=\"0; a4.end\" dur=\"3s\" />");
        append_utf8(&mut svg, b"<animate id=\"a4\" attributeName=\"stop-color\" values=\"#0f2350; #6a5acd\" begin=\"a3.end\" dur=\"3s\" /></stop></linearGradient></defs>");
        append_utf8(&mut svg, b"<style type=\"text/css\">p {font-family: serif; color: white;}</style>");
        append_utf8(&mut svg, b"<rect width=\"400\" height=\"400\" fill=\"url(#grad1)\" />");
        append_utf8(&mut svg, b"<foreignObject x=\"25\" y=\"15\" width=\"350\" height=\"370\"><p class=\"shadow\" xmlns=\"http://www.w3.org/1999/xhtml\">");
        append(&mut svg, get_poem_title(poem_id));
        append_utf8(&mut svg, b"</p><p xmlns=\"http://www.w3.org/1999/xhtml\">");
        append(&mut svg, get_poem_body(poem_id));
        append_utf8(&mut svg, b"</p><p style=\"padding-top: 1em\" xmlns=\"http://www.w3.org/1999/xhtml\">");
        append_utf8(&mut svg, x"e2809520e890a9e58e9f20e69c94e5a4aae9838e");
        append_utf8(&mut svg, b"</p></foreignObject></svg>");
        return svg
    }

    public fun get_poem_svg_base64(poem_id: u64): String {
        let svg = get_poem_svg(poem_id);
        let svg_base64 = utf8(b"data:image/svg+xml;base64,");
        append_utf8(&mut svg_base64, base64::encode(bytes(&svg)));
        return svg_base64
    }

    // ===== Entrypoints =====

    /// Anyone can mint their `SakutaroPoem`!
    public entry fun mint(supply: &mut SakutaroPoemSupply, ctx: &mut TxContext) {
        assert!(supply.total_supply < supply.max_supply, EMaxSupply);
        supply.total_supply = supply.total_supply + 1;

        let id = object::new(ctx);
        let sender = tx_context::sender(ctx);
        let poem_id = get_poem_id(sender);
        let poem_title = get_poem_title(poem_id);
        let poem_svg_base64 = get_poem_svg_base64(poem_id);
        let svg_ipfs_cid = get_poem_svg_ipfs_cid(poem_id);
        let nft = SakutaroPoem { id, poem_id, poem_title, poem_svg_base64, svg_ipfs_cid };
        event::emit(NFTMinted {
            object_id: object::id(&nft),
            creator: sender,
            name: nft.poem_title,
        });
        transfer::public_transfer(nft, sender);
    }

    public entry fun transfer(nft: SakutaroPoem, recipient: address, _: &mut TxContext) {
        nft.poem_id = get_poem_id(recipient);
        nft.poem_title = get_poem_title(nft.poem_id);
        nft.poem_svg_base64 = get_poem_svg_base64(nft.poem_id);
        nft.svg_ipfs_cid = get_poem_svg_ipfs_cid(nft.poem_id);
        transfer::public_transfer(nft, recipient)
    }

    public entry fun burn(nft: SakutaroPoem, _: &mut TxContext) {
        let SakutaroPoem { id, poem_id: _, poem_title: _, poem_svg_base64: _, svg_ipfs_cid: _ } = nft;
        object::delete(id)
    }

    // ===== Tests =====

    #[test]
    fun test_mint() {
        use sui::test_scenario;
        use std::debug;

        let admin = @0xA;
        let user = @0xB;

        let scenario_val = test_scenario::begin(admin);
        let scenario = &mut scenario_val;
        {
            init(SAKUTARO_POEM {}, test_scenario::ctx(scenario));
        };

        test_scenario::next_tx(scenario, admin);
        {
            let supply = test_scenario::take_shared<SakutaroPoemSupply>(scenario);
            mint(&mut supply, test_scenario::ctx(scenario));
            test_scenario::return_shared(supply);
        };

        test_scenario::next_tx(scenario, admin);
        {
            let nft = test_scenario::take_from_sender<SakutaroPoem>(scenario);
            debug::print(&nft.poem_title);
            debug::print(&nft.poem_svg_base64);
            test_scenario::return_to_sender(scenario, nft);
        };

        test_scenario::next_tx(scenario, user);
        {
            let supply = test_scenario::take_shared<SakutaroPoemSupply>(scenario);
            mint(&mut supply, test_scenario::ctx(scenario));
            test_scenario::return_shared(supply)
        };

        test_scenario::next_tx(scenario, user);
        {
            let nft = test_scenario::take_from_sender<SakutaroPoem>(scenario);
            debug::print(&nft.poem_title);
            debug::print(&nft.poem_svg_base64);
            test_scenario::return_to_sender(scenario, nft);
        };

        test_scenario::next_tx(scenario, user);
        {
            let supply = test_scenario::take_shared<SakutaroPoemSupply>(scenario);
            mint(&mut supply, test_scenario::ctx(scenario));
            mint(&mut supply, test_scenario::ctx(scenario));
            mint(&mut supply, test_scenario::ctx(scenario));
            mint(&mut supply, test_scenario::ctx(scenario));
            mint(&mut supply, test_scenario::ctx(scenario));
            mint(&mut supply, test_scenario::ctx(scenario));
            mint(&mut supply, test_scenario::ctx(scenario));
            mint(&mut supply, test_scenario::ctx(scenario));
            test_scenario::return_shared(supply)
        };

        test_scenario::end(scenario_val);
    }
